{
  "name": "answear/poczta-polska-bundle",
  "description": "Poczta Polska integration for Symfony.",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "ext-zip": "*",
    "ext-simplexml": "*",
    "marc-mabe/php-enum": "^3.0",
    "webmozart/assert": "^1.3",
    "symfony/http-kernel": "^4.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^8.0",
    "symfony/phpunit-bridge": "^5.0",
    "phpro/grumphp": "^0.18.0",
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpstan/phpstan": "^0.12.13",
    "phpstan/phpstan-webmozart-assert": "^0.12.2"
  },
  "autoload": {
    "psr-4": {
      "Answear\\PocztaPolskaBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Answear\\PocztaPolskaBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "GrumPHP\\Composer\\DevelopmentIntegrator::integrate"
    ],
    "post-update-cmd": [
      "GrumPHP\\Composer\\DevelopmentIntegrator::integrate"
    ]
  },
  "extra": {
    "grumphp": {
      "config-default-path": "grumphp.yaml"
    }
  }
}
